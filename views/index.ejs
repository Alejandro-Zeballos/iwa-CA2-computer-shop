 <div class="container">
        <div class="row">
            <div class="col-lg-12 text-left">
                <h1 class="mt-5">Welcome to Ale's Computer Shop</h1>
                <p class="lead">Input information of new product</p>
            </div>
        <div id="results" class="col-lg-8 text-center order-3">
            <table class="table">
			    <thead>
				    <tr>
					    <th>Item</th>
                        <th>Type</th>
					    <th>Price</th>
					    <th>Description</th>
                        <th>Specs</th>
					    <th>Action</th>
				    </tr>
			    </thead>
            <thead><tr><th>Products list</tr></th></thead>
			<tbody>
                 <% product.forEach(product =>{ %>
                <tr> 
                    <td><%= product.item %></td>
                    <td><%= product.section %></td>
                    <td><%= product.price %> </td>
                    <td><%= product.desc %> </td>
                    <td><%= product.spec %> </td>
                    <td><button class="btn btn-warning edit-product">Edit</button> <button class="btn btn-danger delete-product">Delete</button><button class="btn btn-success update-product" style="display:none">Update</button> <button class="btn btn-danger cancel" style="display:none">Cancel</button></td>

	</script>
	
	<script src="js/jquery.js"></script>
	<script src="js/underscore/underscore.js"></script>
	<script src="js/backbone.js"></script>
	<script src="js/scripts.js"></script>
</body>
</html>

                    <br>
                 </tr>
             <%})%>
		</table>
        
        </div>